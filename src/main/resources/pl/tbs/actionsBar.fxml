<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="actionsBarController">
   <children>
      <Label text="Password operations" textAlignment="CENTER">
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <HBox>
         <children>
            <Button mnemonicParsing="false" text="Set to:">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <TextField>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" text="&lt; generate random" />
            <Button mnemonicParsing="false" text="Validate">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Print">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Log">
               <HBox.margin>
                  <Insets left="170.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
