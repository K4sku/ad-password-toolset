<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane minWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.tbs.ActionsBarController">
   <children>
      <Label text="Password operations" textAlignment="CENTER" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <HBox AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="20.0">
         <children>
            <Button mnemonicParsing="false" text="Set to:" />
            <TextField>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" text="&lt; generate random" />
            <Button mnemonicParsing="false" text="Validate">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Print">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Button mnemonicParsing="false" text="Log" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="20.0" />
   </children>
   <padding>
      <Insets bottom="5.0" />
   </padding>
</AnchorPane>
