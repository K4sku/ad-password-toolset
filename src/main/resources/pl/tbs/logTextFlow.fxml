   <children>
      <ScrollPane>
         <content>
            <TextFlow fx:id="logTextFlow" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="115.0" prefWidth="688.0" />
         </content>
      </ScrollPane>
      <Button fx:id="popoutButton" mnemonicParsing="false" onAction="#onPopoutButton" text="pop-out" />
   </children>
</StackPane>
